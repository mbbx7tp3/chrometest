{
  "name": "Le wagon chrome extension",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "download.png"
  },
"icons": {
    "16": "download.png",
    "32": "download.png",
    "48": "download.png",
    "128": "download.png"
  }
}
